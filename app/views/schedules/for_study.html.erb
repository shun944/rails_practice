<% provide(:title, "For study") %>
<div class="row">
  <h1>For study</h1>
  <p>
    <center>Test for study!</center>
   
  </p>
  
  <div class="col-md-6 col-md-offset-3"> <%= button_to "Make your Schedule!", for_study_new_path, method: :get, class: "btn btn-primary", size:10 %> </div>
  
  <p>
    <div class="col-md-6 col-md-offset-3">
      <% @schedules.each do |schedule| %>
        <% if schedule.active_flag == false %>
          <%= render 'schedules/schedule', schedule: schedule %>
          <%= will_paginate @schedules %>
        <% end %>
      <% end %>
      
    </div>
  </p>
</div>
