<% provide(:title, "For study") %>
<div class="row">
  <h1>For study</h1>
  
  <div class="col-md-6 col-md-offset-3"> <%= button_to "Make your Schedule!", for_study_new_path, method: :get, class: "btn btn-primary", size:10 %> </div>
  <div class="col-md-6 col-md-offset-3"> <%= button_to "Activeted schedules", for_study_activated_path, method: :get, class: "btn btn-success", size:10 %> </div>
  
  <p>
    <div class="col-md-6 col-md-offset-3">
      <%= render @schedules_f%>
      <%= will_paginate @schedules_f %>
    </div>
  </p>
</div>
